public class Program
{
    public void Main(string[] args)
    {

        
    }  

    public string Fizz(int number)
    {
        var answer = ""

        if(number % 3 == 0)
        {
            answer = "fizz";
        }
        if(number % 5 == 0)
        {
            answer += "buzz";
        }
        
        return answer;
    }


}    